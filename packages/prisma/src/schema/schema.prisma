datasource db {
  provider = "postgresql"
  url      = "postgresql://user:password@db:5432/postgres"
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
