{
  "extends": "@mqs/tsconfig/base.json",
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ],
  "compilerOptions": {
    "target": "ES2016",
    "baseUrl": "./src",
    "lib": [
      "ES2015"
    ],
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "paths": {
      "@": [
        "./"
      ],
      "@/*": [
        "./*"
      ]
    }
  }
}
