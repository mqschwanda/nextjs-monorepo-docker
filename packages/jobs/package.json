{
  "name": "@mqs/jobs",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./cli/cancel": "./dist/cli/cancel.js",
    "./cli/start": "./dist/cli/start.js"
  },
  "typesVersions": {
    "*": {
      "cli/cancel": [
        "./dist/cli/cancel.d.ts"
      ],
      "cli/start": [
        "./dist/cli/start.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "mqs-tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "mqs-tsc-watch --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest"
  },
  "bin": {
    "mqs-jobs-cancel": "./dist/cli/cancel.js",
    "mqs-jobs-start": "./dist/cli/start.js"
  },
  "devDependencies": {
    "@mqs/eslint-config": "*",
    "@mqs/jest-preset": "*",
    "@mqs/prisma": "*",
    "@mqs/tsc": "*",
    "@mqs/tsconfig": "*",
    "@types/node-cron": "^3.0.8",
    "@types/yargs": "^17.0.24",
    "jest": "^29.6.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@mqs/tokens": "*",
    "node-cron": "^3.0.2",
    "yargs": "^17.7.2"
  }
}
