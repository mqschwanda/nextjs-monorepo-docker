{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "scripts/*"
    ]
  },
  "scripts": {
    "build": "turbo run build --concurrency=100%",
    "clean": "turbo run clean --concurrency=100%",
    "codegen": "turbo run codegen --concurrency=100%",
    "docker-build:dev": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker compose -f docker-compose.base.yml -f docker-compose.dev.yml build",
    "docker-build:prod": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker compose -f docker-compose.base.yml -f docker-compose.prod.yml build",
    "docker-run:dev": "docker compose -f docker-compose.base.yml -f docker-compose.dev.yml up -d",
    "docker-run:prod": "docker compose -f docker-compose.base.yml -f docker-compose.prod.yml up -d",
    "docker-kill": "docker kill $(docker ps -q) && docker rm $(docker ps -a -q)",
    "dev": "turbo run dev --filter='./apps/*' --concurrency=100%",
    "lint": "turbo run lint --concurrency=100%",
    "lint:fix": "turbo run lint:fix --concurrency=100%",
    "spelling": "cspell . -c .cspell/cspell.json",
    "test": "turbo run test --concurrency=100%"
  },
  "devDependencies": {
    "@mqs/eslint-config": "*",
    "cspell": "^6.31.2",
    "eslint": "^7.32.0",
    "jest": "^29.6.1",
    "npm-check-updates": "^16.10.16",
    "prettier": "^2.8.8",
    "turbo": "^1.10.9",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.5.1",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "resolutions": {
    "file-system-cache": "2.3.0"
  }
}
