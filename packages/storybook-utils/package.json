{
  "name": "@mqs/storybook-utils",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./initStoryshots": "./dist/initStoryshots/index.js"
  },
  "typesVersions": {
    "*": {
      "initStoryshots": [
        "./dist/initStoryshots/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "yarn clean && mqs-tsc --project tsconfig.build.json",
    "clean": "rm -rfv dist || true",
    "dev": "mqs-tsc-watch --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@storybook/addon-storyshots": "^7.1.1"
  },
  "devDependencies": {
    "@mqs/eslint-config": "*",
    "@mqs/tsc": "*",
    "@mqs/tsconfig": "*",
    "@storybook/react": "^7.1.1",
    "@storybook/types": "^7.1.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "react": "^18.2.0",
    "typescript": "^5.1.6"
  }
}
