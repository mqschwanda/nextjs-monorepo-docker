{
  "name": "@mqs/jest-preset",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "exports": {
    "./node/jest-preset.js": "./dist/node/jest-preset.js",
    "./node/": "./dist/node/",
    "./stubs/css.js": "./dist/stubs/css.js",
    "./utilities": "./dist/utilities/index.js"
  },
  "typesVersions": {
    "*": {
      "node/jest-preset.js": [
        "./dist/node/jest-preset.d.ts"
      ],
      "stubs/css.js": [
        "./dist/stubs/css.d.ts"
      ],
      "utilities": [
        "./dist/utilities/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "yarn clean && mqs-tsc --project tsconfig.build.json",
    "clean": "rm -rfv dist || true",
    "dev": "mqs-tsc-watch --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@babel/preset-env": "^7.22.10",
    "@types/jest": "^29.5.4",
    "babel-jest": "^29.6.4",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@mqs/eslint-config": "*",
    "@mqs/tsc": "*",
    "@mqs/tsconfig": "*",
    "@types/node": "^20.5.6",
    "eslint": "^8.48.0",
    "typescript": "^5.2.2"
  }
}
