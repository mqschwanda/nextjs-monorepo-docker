{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --concurrency=100%",
    "clean": "turbo run clean --concurrency=100%",
    "codegen": "turbo run codegen --concurrency=100%",
    "codegen:watch": "turbo run codegen:watch --concurrency=100%",
    "dev": "turbo run dev --concurrency=100%",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --concurrency=100%",
    "lint:fix": "turbo run lint:fix --concurrency=100%",
    "test": "turbo run test --concurrency=100%"
  },
  "devDependencies": {
    "@mqs/eslint-config": "*",
    "eslint": "^7.32.0",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "turbo": "^1.9.4",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@3.5.1",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
