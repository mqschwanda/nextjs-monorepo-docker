{
  "name": "@mqs/prisma",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client.js"
  },
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client.d.ts"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "generate:schema": "prisma generate --schema ./src/schema/schema.prisma",
    "build": "mqs-tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "mqs-tsc-watch --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@mqs/eslint-config": "*",
    "@mqs/jest-preset": "*",
    "@mqs/tsc": "*",
    "@mqs/tsconfig": "*",
    "eslint": "^7.32.0",
    "jest": "^29.6.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "prisma": "^5.0.0"
  }
}
