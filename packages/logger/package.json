{
  "name": "@mqs/logger",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "browser": "./dist/browser.js",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./browser": "./dist/browser.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "browser": [
        "./dist/browser.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "yarn clean && mqs-tsc --project tsconfig.build.json",
    "clean": "rm -rfv dist || true",
    "dev": "mqs-tsc-watch --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@mqs/eslint-config": "*",
    "@mqs/graphql-schema": "*",
    "@mqs/jest-preset": "*",
    "@mqs/tsc": "*",
    "@mqs/tsconfig": "*",
    "babel-jest": "^29.6.4",
    "eslint": "^8.47.0",
    "jest": "^29.6.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@mqs/errors": "*",
    "@mqs/graphql-client": "*",
    "@mqs/prisma": "*",
    "@prisma/client": "^5.2.0",
    "prisma": "^5.2.0"
  }
}
